		<person id="a78026c1-b3f6-4674-bbe6-99d44cd734e0" type="mekhq.campaign.personnel.Person">
			<name>Ingried</name>
			<callsign></callsign>
			<primaryRole>1</primaryRole>
			<secondaryRole>22</secondaryRole>
